{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars":  
    [
      1, { "args": "after-used", "argsIgnorePattern": "^_" }
  ],
    "prettier/prettier": "warn",
    "no-console": "warn"
  }
}


